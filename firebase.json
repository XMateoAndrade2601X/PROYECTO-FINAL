{
  "firestore": {
    "database": "(default)",
    "location": "nam5",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
   "rewrites": [
  {
    "source": "/registro.html",
    "destination": "/registro.html"
  },
  {
    "source": "/informacion.html",
    "destination": "/informacion.html"
  },
  {
    "source": "/admin.html",
    "destination": "/admin.html"
  },
  {
    "source": "**",
    "destination": "/index.html"
  }
]
  

  }
}
